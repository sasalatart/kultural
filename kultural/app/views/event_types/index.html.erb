<% @title ||= 'Event Types' %> 
<div>
  <h1 class="center-align"> 
    <%= @title %> 
    <% unless @event_type.nil? %>
    <%= link_to @event_type.name, @event_type %>
    <% end %>
  </h1>
</div>
<div class="table-container">
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Examples</th>
      </tr>
    </thead>

    <tbody>
      <% @event_types.each do |event_type| %>
      <tr>
        <td><%= link_to event_type.name, event_type %></td>
        <td><%= event_type.examples %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>