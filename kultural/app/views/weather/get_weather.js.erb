var w = $("#weather");
w.removeClass("loading");
<% if @temp_max != '' %>
w.addClass("loaded");
w.html("<%= j render partial: 'weather/weather', locals: {temp_max: @temp_max, temp_min: @temp_min, weather_uri: @uri, weather_img: @img} %>");
<% else %>
w.addClass("not_found");
w.html("No se encontró información del clima para esta fecha.");
<% end %>
